lib/libarcsoft_magicshot_bestphoto.so:system/lib/libarcsoft_magicshot_bestphoto.so
lib/libarcsoft_magicshot_common.so:system/lib/libarcsoft_magicshot_common.so
lib/libarcsoft_magicshot_drama.so:system/lib/libarcsoft_magicshot_drama.so
lib/libarcsoft_magicshot_eraser.so:system/lib/libarcsoft_magicshot_eraser.so
lib/libarcsoft_magicshot_picmotion.so:system/lib/libarcsoft_magicshot_picmotion.so
lib/libarcsoft_night_shot_ex.so:system/lib/libarcsoft_night_shot_ex.so
lib/libarcsoft_panorama_burstcapture.so:system/lib/libarcsoft_panorama_burstcapture.so

#
# system.prop for picassowifi
#

# Media
ro.sapa.jack.master=alsa
ro.hdcp2.rx=tz

# UI
lockscreen.rot_override=true
ro.sf.lcd_density=320
ro.opengles.version=196609

ro.hwui.texture_cache_size=96
ro.hwui.layer_cache_size=64
ro.hwui.path_cache_size=16
ro.hwui.shape_cache_size=4
ro.hwui.gradient_cache_size=2
ro.hwui.drop_shadow_cache_size=8
ro.hwui.texture_cache_flushrate=0.5
ro.hwui.text_small_cache_width=1024
ro.hwui.text_small_cache_height=512
ro.hwui.text_large_cache_width=2048
ro.hwui.text_large_cache_height=1024

# Fixes game grashing (HeartStone)
debug.egl.force_msaa=true

# Disable persistent GPU compositing
debug.sf.disable_hwc=1

# Radio
ro.radio.noril=yes

# Randomly from stock
ro.ril.gprsclass=10
ro.ril.hsxpa=1
ro.sec.fle.encryption=true
ro.secwvk=220
ro.securestorage.support=true

# Wifi
wifi.interface=wlan0

# Camera
lib/hw/camera.universal5420.so:system/lib/hw/camera.universal5420.so
lib/libexynoscamera.so:system/lib/libexynoscamera.so
lib/libvdis.so:system/lib/libvdis.so
lib/libak09911c.so:system/lib/libak09911c.so
